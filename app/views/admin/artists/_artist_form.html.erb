<%= simple_form_for([:admin, @artist], html: {class: 'form-horizontal'}) do |f| %>
  <%= f.input :name %>
  <%= f.input :featured %>
  <%= f.input :bio %>
  <%= f.input :website %>
  <%= f.input :birthdate %>
  <%= f.input :deathdate %>
  <%= f.input :student %>
  <%= f.input :faculty %>
  <%= f.button :submit %>
<% end %>
