<%= simple_form_for([:admin, @art_piece], html: {class: 'form-horizontal'}) do |f| %>
    <%= f.input :title %>
    <%= f.input :medium %>
    <%= f.input :creation_date %>
    <%= f.input :size %>
    <%= f.input :legal_info %>
    <%= f.input :private %>
    <%= f.input :contact_info %>
    <%= f.input :description %>
    <%= f.input :displayed %>
    <div id="picture">
        <%= f.fields_for :art_piece_photos do |builder| %>
            <%= builder.file_field :photo %> 
        <% end %>
    </div>
    <%= f.button :submit %>
<% end %>
