<% @users.each do |user| %>
  <div class="panel panel-body" id="userstatus-<%= user.id %>">
    <p><b>User Email:</b> <%= user.email %></p>
    <p><b>User Admin Status</b> <% if user.admin %>
    <%= "Admin"%>
    <% else %>
    <%= "User" %>
    <% end %></p>
    <p><%= link_to "Edit", edit_admin_user_path(user) %>
  </div>
<% end %>